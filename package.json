{
  "name": "data-region",
  "version": "0.0.1",
  "description": "Region Data",
  "author": "KILLYOUGUTSXXX",
  "private": true,
  "main": "index.d.ts",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "build": "nest build",
    "start:dev": "SET NEST_ENV=dev&&nest start --watch",
    "start:debug": "NEST_ENV=dev nest start --debug --watch",
    "start:prod": "SET NEST_ENV=prod&&node dist/main",
    "test": "jest --detectOpenHandles --runInBand --forceExit --outputFile ./jest-result --json",
    "test:watch": "NEST_ENV=test jest --watch --detectOpenHandles --runInBand"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.6",
    "@nestjs/config": "^3.2.1",
    "@nestjs/core": "^10.3.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/mongoose": "^10.0.4",
    "@nestjs/platform-express": "^10.3.6",
    "@nestjs/sequelize": "^10.0.1",
    "@prisma/client": "^5.9.1",
    "@types/crypto-js": "^4.2.2",
    "axios": "^1.6.8",
    "cache-manager": "^5.5.1",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "device-detector-js": "^3.0.3",
    "ip": "^2.0.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "mongoose": "^8.2.3",
    "morgan": "^1.10.0",
    "nestjs-rate-limiter": "^3.1.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "short-unique-id": "^5.0.3",
    "ua-parser-js": "^1.0.37"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "20.11.30",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "29.7.0",
    "jest-mock-extended": "^3.0.7",
    "prettier": "^3.2.5",
    "prisma": "^5.9.1",
    "source-map-support": "^0.5.21",
    "superagent-defaults": "^0.1.14",
    "supertest": "^6.3.4",
    "ts-jest": "29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "globalSetup": "<rootDir>/../src/tests/_config/setup.ts",
    "moduleNameMapper": {
      "^@utilities/(.*)$": "<rootDir>/../src/utilities/$1",
      "^@main/(.*)$": "<rootDir>/../src/main/$1",
      "^@app-module": "<rootDir>/../src/app.module.ts",
      "^@test-stubs/(.*)$": "<rootDir>/../src/tests/stubs/$1",
      "^@dtos/(.*)$": "<rootDir>/../src/common/dtos/$1",
      "^@interfaces/(.*)$": "<rootDir>/../src/common/interfaces/$1",
      "^@middlewares$": "<rootDir>/../src/middlewares",
      "^@globals/(.*)$": "<rootDir>/../src/globals",
      "^@fx-routers": "<rootDir>/../src/fx-routers",
      "^@routers/(.*)$": "<rootDir>/../src/routers/$1",
      "^@schemas/(.*)$": "<rootDir>/../src/common/schemas/$1",
      "^src/(.*)$": "<rootDir>/$1"
    }
  },
  "fxrouters": {
    "baseRoute": "<rootDir>/src/routers"
  }
}
